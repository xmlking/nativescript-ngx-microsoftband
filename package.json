{
  "name": "@xmlking/nativescript-ngx-microsoftband",
  "version": "0.1.5",
  "description": "Microsoft Band Plugin for NativeScript with Angular.",
  "main": "index",
  "module": "esm/index",
  "typings": "index.d.ts",
  "nativescript": {
